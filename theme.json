{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1440px"
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#4169e1",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#800080",
					"name": "Secondary"
				},
				{
					"slug": "accent",
					"color": "#ff6b6b",
					"name": "Accent"
				},
				{
					"slug": "success",
					"color": "#0f834d",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#ffa500",
					"name": "Warning"
				},
				{
					"slug": "error",
					"color": "#e2401c",
					"name": "Error"
				},
				{
					"slug": "info",
					"color": "#3d9cd2",
					"name": "Info"
				},
				{
					"slug": "dark",
					"color": "#191970",
					"name": "Dark"
				},
				{
					"slug": "gray-dark",
					"color": "#404040",
					"name": "Gray Dark"
				},
				{
					"slug": "gray",
					"color": "#666666",
					"name": "Gray"
				},
				{
					"slug": "gray-light",
					"color": "#cccccc",
					"name": "Gray Light"
				},
				{
					"slug": "light",
					"color": "#f1f1f1",
					"name": "Light"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				}
			],
			"custom": false,
			"customDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"link": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
					"slug": "system",
					"name": "System Font"
				},
				{
					"fontFamily": "\"Courier 10 Pitch\", courier, monospace",
					"slug": "monospace",
					"name": "Monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base"
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large"
				},
				{
					"slug": "xl",
					"size": "2rem",
					"name": "XL"
				},
				{
					"slug": "xxl",
					"size": "2.5rem",
					"name": "XXL"
				}
			],
			"fontStyle": false,
			"fontWeight": true,
			"letterSpacing": false,
			"textDecoration": false,
			"textTransform": false,
			"lineHeight": true,
			"dropCap": false
		},
		"spacing": {
			"spacingScale": {
				"steps": 7,
				"operator": "*",
				"increment": 1.5,
				"mediumStep": 1.5,
				"unit": "rem"
			},
			"spacingSizes": [
				{
					"slug": "xs",
					"size": "0.5rem",
					"name": "XS"
				},
				{
					"slug": "sm",
					"size": "1rem",
					"name": "SM"
				},
				{
					"slug": "md",
					"size": "1.5rem",
					"name": "MD"
				},
				{
					"slug": "lg",
					"size": "2rem",
					"name": "LG"
				},
				{
					"slug": "xl",
					"size": "3rem",
					"name": "XL"
				},
				{
					"slug": "xxl",
					"size": "4rem",
					"name": "XXL"
				}
			],
			"units": [ "rem", "px", "em", "%", "vh", "vw" ],
			"padding": true,
			"margin": true,
			"blockGap": true
		},
		"custom": {
			"typography": {
				"fontSize": {
					"custom": false
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "1.5"
		},
		"spacing": {
			"padding": {
				"top": "0",
				"right": "0",
				"bottom": "0",
				"left": "0"
			},
			"blockGap": "1.5rem"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxl)",
					"lineHeight": "1.2"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--md)"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)",
					"lineHeight": "1.2"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--md)"
					}
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.3"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--sm)"
					}
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.3"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--sm)"
					}
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"lineHeight": "1.4"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--xs)"
					}
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.4"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--xs)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--dark)"
					}
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--dark)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			}
		}
	}
}
